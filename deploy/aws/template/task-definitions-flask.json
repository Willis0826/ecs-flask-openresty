[
    {
        "name": "flask",
        "image": "990090895087.dkr.ecr.us-east-2.amazonaws.com/ecs-flask-openresty/flask:{{.Env.VERSION}}",
        "cpu": 10,
        "memory": 256,
        "essential": true,
        "command": [
            "sh",
            "-c",
            "echo 1 && python app.py"
        ],
        "portMappings": [
            {
                "containerPort": 5000,
                "hostPort": 0
            }
        ]
    }
]
